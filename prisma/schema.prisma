generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Tutor {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  phone       String
  email       String
  dateOfBirth DateTime
  zipCode     String
}

model Pet {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  species     String
  carry       String
  weight      Float
  dateOfBirth DateTime
}
